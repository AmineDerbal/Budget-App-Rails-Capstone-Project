
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class='header'>
<%= link_to root_path do%>
<%= image_tag('arrow-118-32.png', class:'icon') %>
<%end%>
<h2>LOGIN</h2>
 <div class="actions">
      <%= f.submit "Log in", class:'login' %>
    </div>
</div>

<div class="form-container">
    <div class="field">
      <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "Email", class:'form-control' %>
    </div>

    <div class="field">
      <%= f.password_field :password, autocomplete: "current-password", placeholder: "Password", class:'form-control' %>
    </div>

    <% if devise_mapping.rememberable? %>
      <div class='cancel-center' >
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      </div>
    <% end %>

   
  <% end %>

  <%= render "devise/shared/links" %>
</div>